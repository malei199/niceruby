.man_user_list .man_search {
    padding-top: 16rpx;
    height: 72rpx;
    background: #F4F4F4;
    border-bottom: 1px solid #E5E5E5;
}

.man_user_list .man_search .index_search_box {
    margin: 0 auto;
    display: block;
    width: 718rpx;
    height: 56rpx;
    background-color: #fff;
    border-radius: 6rpx;
    line-height: 60rpx;
    text-align: center;
    color: #B2B2B2;
    font-size: 26rpx;
}

.man_user_list .man_search .index_search_box .index_search_icon {
    display: inline-flex;
    vertical-align: sub;
}

.man_user_list .man_location {
    padding-left: 30rpx;
    font-size: 28rpx;
    color: #888888;
    line-height: 82rpx;
}

.man_user_list .man_location .jt_img {
    margin-top: 34rpx;
    margin-left: 10rpx;
    width: 20rpx;
    height: 14rpx;
    display: inline-flex;
}

.man_user_list .man_List .man_list_header {
    display: block;
    padding-left: 24rpx;
    font-size: 24rpx;
    color: #888;
    line-height: 66rpx;
    background: #F4F4F4;
}

.man_user_list .man_List .man_list_header em {
    color: #ED5566;
}

.man_user_list .man_List .man_list_ul li {
    padding: 0 30rpx 36rpx;
    display: block;
    border-bottom: 1px solid #E5E5E5;
}

.man_user_list .man_List .man_list_ul li:last-child {
    border-bottom: none;
}

.man_user_list .man_list_title {
    margin-top: 24rpx;
    display: block;
    line-height: 50rpx;
    font-size: 32rpx;
    color: #353535;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.man_user_list .man_list_title .grade {
    float: left;
    margin-right: 13rpx;
    padding: 0 13rpx;
    font-size: 20rpx;
    line-height: 50rpx;
    border-radius: 3px;
}

.man_user_list .man_list_title .grade.gradeStyle1 {
    color: #FF4C13;
    background: #FFE1D6;
}

.man_user_list .man_list_title .grade.gradeStyle2 {
    color: #EB8600;
    background: #FFEFD6;
}

.man_user_list .man_list_title .grade.gradeStyle3 {
    color: #BEA200;
    background: #FFF1D6;
}

.man_user_list .man_list_con {
    overflow: hidden;
}

.man_user_list .man_list_con span {
    float: left;
    overflow: hidden;
    width: 450rpx;
    font-size: 28rpx;
    line-height: 37rpx;
    color: #888888;
}

.man_user_list .man_list_con .address {
    margin-top: 26rpx;
}

.man_user_list .man_list_con .name {
    margin-top: 25rpx;
}

.man_user_list .man_list_con .price {
    width: 168rpx;
    float: right;
    margin-top: 27rpx;
    margin-bottom: 8rpx;
    line-height: 36rpx;
    font-size: 22rpx;
    text-align: center;
}

.man_user_list .man_list_con .address image {
    float: left;
    margin-top: 7rpx;
    margin-right: 14rpx;
    width: 23rpx;
    height: 28rpx;
}

.man_user_list .man_list_con .name image {
    margin-top: 7rpx;
    margin-right: 14rpx;
    float: left;
    width: 25rpx;
    height: 25rpx;
}

.man_user_list .man_list_con .btn {
    margin-top: 33rpx;
    width: 16rpx;
    height: 26rpx;
    float: right;
}